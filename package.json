    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.10",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.5",
    "@types/sequelize": "^4.28.9",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
